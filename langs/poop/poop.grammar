  compile() {
    let day = ""
    const rows = this.getTopDownArray().map(node => {
      if (node.doesExtend("dayNode")) {
        day = node.getLine()
            return undefined
              }return node.compile(day)
      }).filter(row => row)
    return `date,time,event,notes\n` + rows.join("\n")
  }
