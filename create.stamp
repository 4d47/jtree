folder src/
file src/LANGProgram.js
 data
  const TreeProgram = require("treeprogram")
  const fs = require("fs")
  const LANGGrammar = fs.readFileSync(__dirname + "/LANG.grammar", "utf8")
  
  class LANGProgram extends TreeProgram {
    getGrammarString() {
      return LANGGrammar
    }
  
    getGrammarFilePath() {
      return __dirname + "/LANG.grammar"
    }
  }
  
  module.exports = LANGProgram
file src/LANG.grammar
 data
  LANG
   @description DESCRIPTION
   @catchAllKeyword error
   @keywords
    root
  root
  error
   @parseClass ErrorNode
